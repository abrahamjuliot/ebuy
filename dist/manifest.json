{
  "name": "eBuy",
  "version": "1.0.1",
  "description": "UCR eBuy Receiver Extension",
  "permissions": ["activeTab", "declarativeContent", "storage"],
  "content_scripts": [
    {
      "matches": ["https://ebuy.ucr.edu/*"],
      "css": ["style.min.css"],
      "js": ["app.js"],
      "match_about_blank": true,
      "all_frames": true
    }
  ],
  "icons": { "48": "icon48.png"},
  "manifest_version": 2
}
